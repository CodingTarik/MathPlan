{
  "enableDefaultPreprocessing": true,
  "additionalPreprocessing": [
    {
      "find": "Leistungspun kte",
      "flags": "g",
      "replace": "Leistungspunkte"
    },
    {
      "find": "\\uf0b7",
      "flags": "g",
      "replace": "â€¢"
    }
  ],
  "moduleTitle": "Modulbeschreibung",

  "moduleProperties": [
    {
      "propertyName": "moduleID",
      "readFrom": "\\d{2}-\\d{2}-",
      "readTo": "\\d{4}(\\/\\w+)?",
      "keepReadFromAndTo": true,
      "useResultAtIndex": 0,
      "postprocessing": [
        {
          "find": "-\\s.*(\\d{4})",
          "flags": "g",
          "replace": "-$1"
        }
      ]
    },
    {
      "propertyName": "moduleName",
      "readFrom": "Modulname",
      "readTo": "Modul Nr\\.",
      "keepReadFromAndTo": false,
      "useResultAtIndex": 0,
      "postprocessing": []
    },
    {
      "propertyName": "moduleCredits",
      "readFrom": "\\d+ CP",
      "readTo": "",
      "keepReadFromAndTo": true,
      "useResultAtIndex": 0,
      "postprocessing": []
    },
    {
      "propertyName": "moduleLanguage",
      "readFrom": "Modulname",
      "readTo": "Modulverantwortliche Person",
      "keepReadFromAndTo": false,
      "useResultAtIndex": 0,
      "postprocessing": [
        {
          "find": ".*Sprache\\s*",
          "flags": "g",
          "replace": ""
        }
      ]
    },
    {
      "propertyName": "moduleApplicability",
      "readFrom": "8 Verwendbarkeit des Moduls",
      "readTo": "9 Literatur",
      "keepReadFromAndTo": false,
      "useResultAtIndex": 0,
      "postprocessing": []
    }
  ]
}